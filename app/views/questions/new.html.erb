<h1>Questions#new</h1>

<%= form_with url: user_questions_path, local: true do |form| %>
    <%= form.label :title%>
    <%= form.text_field :title %>
    <%= form.label :body %>
    <%= form.text_area :body %>
    <%= form.submit %>
    <%= form.hidden_field :username, :value => current_user.username %>
<% end %>